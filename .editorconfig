; top-most EditorConfig file
root = true

; Unix-style newlines
[*]
end_of_line = LF
insert_final_newline = true

[*.php]
indent_style = space
indent_size = 4

[*.{yml,yaml}]
indent_style = space
indent_size = 4

[*.feature]
indent_style = space
indent_size = 4


[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
